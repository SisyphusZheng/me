{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "types": ["preact", "preact/hooks", "preact/jsx-runtime"],
    "paths": {
      "$fresh/*": ["./node_modules/@types/fresh/*"],
      "preact": ["./node_modules/preact"],
      "preact/*": ["./node_modules/preact/*"],
      "@preact/signals": ["./node_modules/@preact/signals"],
      "@preact/signals-core": ["./node_modules/@preact/signals-core"],
      "twind": ["./node_modules/twind"],
      "twind/*": ["./node_modules/twind/*"],
      "marked": ["./node_modules/marked"],
      "highlight.js": ["./node_modules/highlight.js"],
      "marked-highlight": ["./node_modules/marked-highlight"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
